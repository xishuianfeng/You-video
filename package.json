{
  "name": "video-player",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf dist",
    "build:win": "npm run clean && npm run build && electron-builder --win --config electron-builder-config.js",
    "build:mac": "npm run clean && electron-vite build && electron-builder --mac --config electron-builder-config.js",
    "build:linux": "npm run clean && electron-vite build && electron-builder --linux --config electron-builder-config.js"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/wicg-file-system-access": "^2020.9.6",
    "@videojs/themes": "^1.0.1",
    "ahooks": "^3.7.7",
    "classnames": "^2.3.2",
    "electron-better-ipc": "^2.0.1",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-reloader": "^1.2.3",
    "electron-updater": "^5.3.0",
    "eventemitter3": "^4.0.7",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "immer": "^10.0.2",
    "lowdb": "^6.0.1",
    "peerjs": "^1.4.7",
    "react-router-dom": "^6.11.2",
    "rip-subtitles": "^0.0.1",
    "sass": "^1.63.3",
    "subtitle": "^4.2.1",
    "use-immer": "^0.9.0",
    "uuid": "^9.0.0",
    "video.js": "^8.3.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@icon-park/react": "^1.4.2",
    "@types/node": "16.18.21",
    "@types/react": "18.0.30",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "18.0.11",
    "@types/react-modal": "^3.16.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.21",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "node-ffprobe@1.2.2": "patches/node-ffprobe@1.2.2.patch"
    }
  }
}
